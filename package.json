{
  "name": "nuxt-layer-template",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.4",
  "license": "MIT",
  "workspaces": [
    "base",
    "app",
    "auth"
  ],
  "scripts": {
    "dev": "pnpm --filter ./app dev",
    "build": "pnpm --filter ./app build",
    "start": "pnpm --filter ./app start",
    "lint": "pnpm --filter ./app lint",
    "auth:lint": "pnpm --filter ./auth lint",
    "auth:dev": "pnpm --filter ./auth dev",
    "base:lint": "pnpm --filter ./base lint",
    "base:dev": "pnpm --filter ./base dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@eslint/eslintrc": "^3.0.2",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@typescript-eslint/parser": "^7.1.1",
    "@wisemen/eslint-config-vue": "^0.0.10",
    "eslint": "^8.57.0",
    "eslint-plugin-check-file": "2.7.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-perfectionist": "^2.6.0",
    "eslint-plugin-putout": "^22.4.1",
    "eslint-plugin-require-explicit-generics": "1.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3"

  }
}
